<h1>Tasks</h1>
<ul>
  <% @tasks.each do |task|  %>
    <li>
      <%= link_to(task.name, task_path(task.id) )%>
<!--      Description: <%#= task.description %>-->
      <%= link_to("Edit Task", edit_task_path(task.id) )%>
      <%= link_to "Delete Task", task_path(task.id), method: :delete %>
    </li>
  <% end %>

  <li> <%= link_to "Add another task", new_task_path %> </li>
</ul>


