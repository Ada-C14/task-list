
<main>
  <ul class = items>
    <% @tasks.each do |task|  %>
      <li>
        <h4><%= link_to task.name, task_path(task.id) %> </h4>
        <p> Description: <%= task.description %></p>
        <p> Date completed: <%=display_task_completed_at(task)%> </p>
      </li>
    <% end %>
  </ul>
</main>