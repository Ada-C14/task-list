<h1>All Tasks</h1>
<ol>
  <% @tasks.each do |task| %>
    <li>
      <%= task[:name] %> -
      <%= task[:description] %> |
      <%#= task[:completed_at].nil? ? "in-progress" : "complete" %>
    </li>
  <% end %>
</ol>
<button><% link_to "New task", new_task_path %>Add Task</button>

