<h1>Tasks</h1>
<ul>
  <% @tasks.each do |task|  %>
    <li>
      <a href="/tasks/<%=task.id %>"> Name:</strong>  <%= task.name %> </a>
      <strong>Description:</strong> <%= task.description %>
      <strong>Completed at:</strong> <%= task.completed_at%>
    </li>
  <% end %>

  <li> 
  <%= link_to "New Task", new_task_path %>
  </li>
</ul>